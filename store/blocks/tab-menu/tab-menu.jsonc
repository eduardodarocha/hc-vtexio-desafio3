{
  "sticky-layout#hc-upmedal-menu": {
  "children": ["tab-layout#header"]
},

"tab-layout#header": {
  "props": {
    "blockClass": "tabheader",
    "defaultActiveTabId": "tabheader1",
    "horizontalAlign": "center",
    "verticalAlign": "center",
    "preventHorizontalStretch": true,
    "preventVerticalStretch": true,
    "fullWidth": true
  },
  "children": [     
    "tab-list#header",
    "tab-content#header"
  ]
},

"tab-list#header": {
  "children": [
    "tab-list.item#header1",
    "tab-list.item#header2",
    "tab-list.item#header3",
    "tab-list.item#header4",
    "tab-list.item#header5",
    "tab-list.item#header6",
    "tab-list.item#header7"
  ]
},
"tab-list.item#header1": {
  "props": {
    "tabId": "header1",
    "label": "PROMOÇÃO",
    "defaultActiveTab": true
  }
},
"tab-list.item#header2": {
  "props": {
    "tabId": "header2",
    "label": "CORINTHIANS"
  }
},
"tab-list.item#header3": {
