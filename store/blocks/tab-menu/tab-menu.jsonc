    "label": "DIVERTIDOS"
  }
},
"tab-list.item#header6": {
  "props": {
    "tabId": "header6",
    "label": "7MARAVILHAS"
  }
},
"tab-list.item#header7": {
  "props": {
    "tabId": "header7",
    "label": "GUERREIROS"
  }
},
"tab-content#header": {
  "children": [
    "tab-content.item#header1",
    "tab-content.item#header2",
    "tab-content.item#header3",
    "tab-content.item#header4",
    "tab-content.item#header5",
    "tab-content.item#header6",
    "tab-content.item#header7"
  ]
},
"tab-content.item#header1": {
  "children": [
    "search-result-layout.customQuery#promocao"   
